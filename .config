{
    "keys":
    [
        {
            "name":     "check",
            "alias":    "-c",
            "args":     "[<profile>]",
            "short":    "Check profile against local git config",
            "doc":      "Check if 'git config user.name' and 'git config user.email' match the given user profile. If no argument is given, just output the current user profile in local git config."
        },

        {
            "name":     "switch",
            "alias":    "-s",
            "args":     " <profile>",
            "short":    "Switch user profile",
            "doc":      "Given the input profile, switch the local user.name and user.email in local git config."
        },

        {
            "name":     "list",
            "alias":    "-l",
            "args":     null,
            "short":    "List all registered user profiles",
            "doc":      "Parse .registry for user profiles and print them."
        },

        {
            "name":     "help",
            "alias":    "-h",
            "args":     "[<command>]",
            "short":    "Display help",
            "doc":      "Display help for the given command."
        }
    ]
}